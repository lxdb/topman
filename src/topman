        E_8_5 = builder.get_object("Status_8")
        E_9_5 = builder.get_object("Status_9")
        
        
        global s_zahl_0
        connection = sqlite3.connect(db)
        cursor= connection.cursor()
        sql = "SELECT * FROM Devices"
        cursor.execute(sql)
        
        text=""
        column_x=[]
        row_x=[]
    
        i=0
        for row in cursor:
        


            r0=str(row[0])
            r1=str(row[1])
            r2=str(row[2])
            r3=str(row[3])
            r4=str(row[4])
            r5=str(row[5])
        
        
            column_x = [(r0, r1, r2, r3, r4, r5)]
            row_x.extend(column_x)
        

    
        connection.commit()
        connection.close()
        print("len(column_x)", len(column_x))
        print("len(row_x)", len(row_x))
        x=len(row_x)

