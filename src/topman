            r0=str(row[0])
            r1=str(row[1])
            r2=str(row[2])
            r3=str(row[3])
                   
        
            column_x = [(r0, r1, r2, r3)]
            row_x.extend(column_x)
        

    
        connection.commit()
        connection.close()
        print("len(column_x)", len(column_x))
        print("len(row_x)", len(row_x))
        x=len(row_x)

